                                      \"CAEN/QC6mainframe/board01/channel006.actual.status\");
  dyn_int statusInts;
  dpGet( statusDpes , statusInts);
  int new_status = 0;
  i=1;
  for (int i=1; i<=7 ; i++)
  {
    new_status = new_status | statusInts[i];
  }
  dpSet(\"MC_SC1top_overview.actual.status\", new_status);
}

